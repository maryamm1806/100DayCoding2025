public class day28 {
    public static void main(String[] args) {
        int a = 10;
        int b = 20;

        // Menggunakan operator ==
        System.out.println("Apakah a sama dengan b? " + (a == b));

        // Menggunakan operator !=
        System.out.println("Apakah a tidak sama dengan b? " + (a != b));
    }
}
