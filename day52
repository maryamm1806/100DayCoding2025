import java.util.Scanner;

public class day52 {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int i = 1;
        System.out.print("masukkan batas angka : ");
        int batas = input.nextInt();

        while (i <= batas) {
            System.out.println("perulangan ke-" + i);
            i++;

        }

    }
}
