public class Main {
    public static void main(String[] args) {
        int angka1 = 17;
        int angka2 = 5;

        // Operasi modulus
        int sisaBagi = angka1 % angka2;

        System.out.println("Angka 1: " + angka1);
        System.out.println("Angka 2: " + angka2);
        System.out.println("Sisa bagi (modulus): " + sisaBagi);
    }
}
