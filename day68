import java.util.Scanner;

public class day68 {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

        System.out.print("masukkan ukuran : ");
        int  m = input.nextInt();

        for (int i = 1; i <= m; i++) {
            for (int j = 1; j <= m; j++) {
                System.out.print(" * ");
            }
            System.out.println();
        }
    }
}
