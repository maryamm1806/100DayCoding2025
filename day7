public class Day7 {
    public static void main(String[] args) {
        char gender = 'P'; // tipe  data char cuman bisa satu karakter (huruf,angka) dan harus pake petik koma ' '
        boolean benar =  true; // tipe data boolean cuman ada dua karakter yaitu(true dan false)

        System.out.println("Gender : " + gender);
        System.out.println("Benar : " + benar); 
    }
}
