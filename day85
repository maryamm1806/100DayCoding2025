public class day85 {
    public static void main(String[] args) {
        int [] angka = {12, 45, 7, 89, 23, 56};
        int max = angka[0];
        for (int i = 1; i < angka.length; i++) {
            if(angka[i] > max) {
                max = angka[i];
            }
            }
        System.out.println("angka maksimal dalam array adalah : " + max);
        }
    }

