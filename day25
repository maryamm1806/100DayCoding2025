public class day25 {
    public static void main(String[] args) {
        System.out.println("perulangan naik : ");
        for (int i = 1; i <= 5; i++) {
            System.out.println("nilai i : " + i);
        }
        System.out.println("\nperulangan turun : ");
        for (int i = 5; i >=  1; i--) {
            System.out.println("nilai i : " + i);
        }
    }
}
