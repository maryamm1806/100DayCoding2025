public class Day15 {
public static void main(String[] args) {

int angka1 = 10;    
    int angka2 = 5;     

    // penjumlahan  
    int hasilTambah = angka1 + angka2;  

    // pengurangan  
    int hasilKurang = angka1 - angka2;  

    // Menampilkan hasil  
    System.out.println("Angka 1: " + angka1);  
    System.out.println("Angka 2: " + angka2);  
    System.out.println("Hasil jumlah: " + hasilTambah);  
    System.out.println("Hasil kurang: " + hasilKurang);  
}

}



