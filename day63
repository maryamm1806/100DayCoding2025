import java.util.Scanner;

public class day63 {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

        System.out.println("masukkan nilai N : ");
        int n = input.nextInt();

        int hasil = 1;

        for (int i = 1; i < n; i++) {
            hasil = hasil * i;
        }
        System.out.println("hasil perkalian dari 1 sampai " + n + " adalah : " + hasil);
    }
}
